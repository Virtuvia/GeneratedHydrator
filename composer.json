{
    "name":              "ocramius/generated-hydrator",
    "description":       "An Object Hydrator that allows very fast array to object to array conversion",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/Ocramius/GeneratedHydrator",
    "keywords":          [
        "hydrator",
        "performance",
        "array conversion",
        "object conversion",
        "serializer"
    ],
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require": {
        "php": "^7.4.7 | ^8.0",
        "laminas/laminas-hydrator": "^4.5",
        "nikic/php-parser": "^4.10.4",
        "ocramius/code-generator-utils": "^1.1.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2.0",
        "infection/infection": "^0.20.2",
        "laminas/laminas-filter": "^2.9.4",
        "laminas/laminas-servicemanager": "^3.17",
        "laminas/laminas-stdlib": "^3.2.1",
        "phpbench/phpbench": "^0.17.1",
        "phpunit/phpunit": "^9.5.1",
        "psalm/plugin-phpunit": "^0.13.0",
        "roave/backward-compatibility-check": "^5.0.0",
        "squizlabs/php_codesniffer": "^3.5.8",
        "vimeo/psalm": "4.6.4"
    },
    "autoload": {
        "psr-4": {
            "GeneratedHydrator\\": "src/GeneratedHydrator"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneratedHydratorBenchmark\\": "benchmarks/GeneratedHydratorBenchmark",
            "GeneratedHydratorTest\\":      "tests/GeneratedHydratorTest",
            "GeneratedHydratorTestAsset\\": "tests/GeneratedHydratorTestAsset"
        }
    },
    "config": {
        "sort-packages": true
    }
}
